<script type="text/javascript">
function addScript(p) {
  var element = document.createElement("script");
  element.src = p;
  document.body.appendChild(element);
}

function fetchJS() {
addScript("/js/lazy.js");
//addScript("/js/plugins/qtip2/jquery.qtip.min.js");
//addScript("/js/plugins/prettyPhoto/js/jquery.prettyPhoto.js");
//addScript("/js/colorbox/1.5.5/jquery.colorbox-min.js");
}

if (window.addEventListener)
  window.addEventListener("load", fetchJS, false);
else if (window.attachEvent)
  window.attachEvent("onload", fetchJS);
else 
  window.onload = fetchJS;
</script>
</body></html>

